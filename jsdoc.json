{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(test|spec)\\.js$"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}