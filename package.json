{
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jsdoc": "^4.0.5",
    "prettier": "^3.7.4",
    "vitest": "^4.0.16"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "test": "vitest",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "jsdoc -c jsdoc.json && npx http-server docs -o"
  }
}
